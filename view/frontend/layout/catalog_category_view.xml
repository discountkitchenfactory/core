	<body>
<!-- 2018-11-18
"Remove the `CleverSoft_CleverCookieLaw` module from category pages:
it should improve the Google PageSpeed score": https://github.com/discountkitchenfactory/core/issues/13 -->
		<referenceBlock name='authentication-popup' remove='true'/>
		<referenceBlock name='cleversoft.js.theme' remove='true'/>
		<referenceBlock name='cleversoft.lazyload' remove='true'/>
		<referenceBlock name='cookielaw.after_body_start' remove='true'/>
		<referenceBlock name='cookielaw.before_body_end' remove='true'/>
		<referenceBlock name='customer.customer.data' remove='true'/>
		<referenceBlock name='customer.data.invalidation.rules' remove='true'/>
		<referenceBlock name='customer.section.config' remove='true'/>
		<referenceBlock name='frontend-storage-manager' remove='true'/>
		<referenceBlock name='google_analytics' remove='true'/>
		<referenceBlock name='head.polyfill' remove='true'/>
		<referenceBlock name='header' remove='true'/>
		<referenceBlock name='minicart' remove='true'/>
		<referenceBlock name='quickview.theme.scripts' remove='true'/>
		<referenceBlock name='require-cookie' remove='true'/>
		<referenceContainer name='category.product.list.additional' remove='true'/>
		<referenceContainer name='page.messages' remove='true'/>
		<referenceContainer name='sidebar.main' remove='true'/>
	</body>
</page>